{
  "private": true,
  "type": "module",
  "workspaces": [
    "blocks/*",
    "packages/*",
    "apps/*/controller"
  ],
  "scripts": {
    "appsemble": "ts-node-esm -T packages/cli/index.ts",
    "create-appsemble": "ts-node-esm -T packages/create-appsemble/index.ts",
    "e2e": "npm --workspace @appsemble/e2e run e2e",
    "prepare": "husky install || true",
    "prettier": "prettier --check",
    "scripts": "ts-node-esm -T packages/scripts/index.ts",
    "start": "ts-node-esm -T packages/cli/index.ts start --remote https://appsemble.app",
    "stylelint": "stylelint .",
    "test": "vitest"
  },
  "lint-staged": {
    "*.css": [
      "stylelint --report-needless-disables"
    ],
    "*.md": [
      "remark --frail --no-stdout"
    ],
    "*.{html,json,md,scss,yaml,yml}": [
      "prettier --check"
    ],
    "*.{js,md,ts,tsx}": [
      "eslint"
    ],
    "**/{messages.ts,package.json,tsconfig.json,vitest.config.js,LICENSE.md,packages/{cli/assets/appsemblerc.schema.json,types/cli.ts}}": [
      "ts-node-esm -T packages/scripts/index.ts validate"
    ]
  },
  "overrides": {
    "axios": "~1.2.0"
  },
